<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="439.0" prefWidth="717.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ir.maxivityteam.touchpainting.MainController">
    <children>
      
      <Button fx:id="connect" defaultButton="true" layoutX="643.0" layoutY="14.0" mnemonicParsing="false" text="Connect" />
      <ComboBox fx:id="list" layoutX="209.0" layoutY="14.0" prefHeight="25.0" prefWidth="189.0" promptText="Select Touchpad Port" />
      <TextArea fx:id="area" editable="false" layoutX="14.0" layoutY="47.0" prefHeight="378.0" prefWidth="687.0" />
      <ComboBox fx:id="switchModeComboBox" disable="true" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="189.0" promptText="Select Touchpad Mode" />
      <Button fx:id="switchmodeButton" disable="true" layoutX="465.0" layoutY="14.0" mnemonicParsing="false" text="Switch Mode" />
      <Button fx:id="disconnect" cancelButton="true" layoutX="562.0" layoutY="14.0" mnemonicParsing="false" text="Disconnect" />
    </children>
</AnchorPane>
